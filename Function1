package by.module1.main;

 /*Найдите значение функции : z = ((a=3)*b/2)+c  */
 
public class Func1 {
    public static void main(String args[]){
        double a = 2;
        double b = 3;
        double c = 1;

        double z = ((a - 3) * b/2) + c;
        System.out.println("При a = " + a + "b = " + b + "c = " + c + "значение функции z  =" + z);
    }
}
